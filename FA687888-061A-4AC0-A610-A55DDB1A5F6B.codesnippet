	<key>IDECodeSnippetContents</key>
	<string>        myView.translatesAutoresizingMaskIntoConstraints = false
        addSubview(myView)
        
        let constraints: [NSLayoutConstraint] = [
            myView.topAnchor.constraintEqualToAnchor(myView.superview?.topAnchor),
            myView.bottomAnchor.constraintEqualToAnchor(myView.superview?.bottomAnchor),
            myView.leftAnchor.constraintEqualToAnchor(myView.superview?.leftAnchor),
            myView.rightAnchor.constraintEqualToAnchor(myView.superview?.rightAnchor)
        ]
        
        constraints[0].priority = UILayoutPriorityDefaultHigh
        constraints[1].priority = UILayoutPriorityDefaultHigh
        
        NSLayoutConstraint.activateConstraints(constraints)</string>
